{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 200,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "bracketSameLine": true,
  "proseWrap": "preserve"
}
