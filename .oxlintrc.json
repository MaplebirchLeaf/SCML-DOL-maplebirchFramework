{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "ignorePatterns": ["node_modules/", "dist/", "publish/", "temp/", ".cache/"],
  "plugins": ["import", "jsdoc", "node", "oxc", "promise", "typescript", "unicorn"],
  "rules": {
    "no-implied-eval": "off",
    "no-extra-boolean-cast": "off",
    "no-unused-vars": "off"
  }
}
